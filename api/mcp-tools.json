{
  "tools": [
    {
      "name": "get_timeseries",
      "description": "Fetch timeseries data for a tag_id",
      "input_schema": {
        "type": "object",
        "properties": {
          "tag_id": { "type": "string" },
          "start": { "type": "string", "format": "date-time" },
          "end": { "type": "string", "format": "date-time" },
          "limit": { "type": "integer", "minimum": 1, "maximum": 5000 }
        },
        "required": ["tag_id"]
      }
    }
  ]
}
